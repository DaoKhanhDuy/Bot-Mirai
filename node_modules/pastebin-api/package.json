{
  "name": "pastebin-api",
  "version": "4.0.1",
  "description": "A very simple npm package to interact with the pastebin api.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Dev-CasperTheGhost",
    "url": "https://www.caspertheghost.me"
  },
  "scripts": {
    "test": "npx ts-node ./tests/index.test.ts",
    "format": "prettier --write \"./**/**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "test-tsc": "tsc --noEmit",
    "build": "rimraf dist/ && tsup"
  },
  "keywords": [
    "pastebin",
    "hastebin",
    "paste",
    "pastebin-api"
  ],
  "dependencies": {
    "fast-xml-parser": "^4.0.7",
    "undici": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api"
  },
  "bugs": {
    "url": "https://github.com/Dev-CasperTheGhost/pastebin-api/issues"
  },
  "homepage": "https://github.com/Dev-CasperTheGhost/pastebin-api#readme",
  "devDependencies": {
    "@casper124578/eslint-config": "^4.3.0",
    "@types/node": "^17.0.30",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.6",
    "typescript": "^4.6.4"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2
  }
}
